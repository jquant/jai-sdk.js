{
    "name": "jai-sdk",
    "version": "0.1.0",
    "description": "JAI SDK for node",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "tsnode": "ts-node",
        "build": "del lib && tsc",
        "format": "prettier --write \"src/**/*.(js|ts)\"",
        "lint": "eslint src --ext .js,.ts",
        "lint:fix": "eslint src --fix --ext .js,.ts",
        "test": "jest --config jest.config.js --detectOpenHandles",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jquant/jai-sdk.js.git"
    },
    "keywords": [
        "boilerplate",
        "typescript"
    ],
    "author": "Alzemiro Iago de Moraes Thomaz",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jquant/jai-sdk.js.git/issues"
    },
    "homepage": "https://github.com/jquant/jai-sdk.js.git#readme",
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/node": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "eslint": "^7.11.0",
        "eslint-plugin-jest": "^24.1.0",
        "jest": "^26.6.0",
        "ts-jest": "^26.4.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2"
    },
    "files": [
        "lib/**/*"
    ],
    "dependencies": {
        "@techmmunity/utils": "^1.10.0",
        "@types/cli-progress": "^3.9.2",
        "axios": "^0.24.0",
        "cli-progress": "^3.9.1",
        "dataframe-js": "^1.4.4"
    }
}